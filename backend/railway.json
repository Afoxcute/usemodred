{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "dockerfile",
    "dockerfilePath": "./Dockerfile"
  },
  "deploy": {
    "startCommand": "yarn start",
    "healthcheckPath": "/health",
    "healthcheckTimeout": 100
  },
  "env": {
    "PORT": {
      "type": "runtime",
      "value": "3000"
    }
  }
} 